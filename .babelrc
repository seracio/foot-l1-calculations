{
    "plugins": [
        "transform-flow-strip-types",
        "transform-object-rest-spread"
    ],
    "env": {
        "test": {
            "presets": [
                [
                    "env",
                    {
                        "targets": {
                            "node": "current"
                        }
                    }
                ]
            ]
        },
        "production": {
            "presets": [
                [
                    "env",
                    {
                        "targets": {
                            "browsers": [
                                "> 5% in FR"
                            ]
                        },
                        "modules": false
                    }
                ]
            ]
        }
    }
}